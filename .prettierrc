{
  "printWidth": 100,
  "tabWidth": 4,
  "useTabs": false,
  "semi": false,
  "parser": "babylon",
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": { "parser": "json", "trailingComma": "none" }
    },
    {
      "files": ".babelrc",
      "options": { "parser": "json", "trailingComma": "none" }
    },
    {
      "files": "*.ts",
      "options": { "parser": "typescript", "trailingComma": "none" }
    },
    {
      "files": "*.tsx",
      "options": { "parser": "typescript", "trailingComma": "none" }
    },
    {
      "files": "*.json",
      "options": { "parser": "json", "trailingComma": "none" }
    }
  ]
}
